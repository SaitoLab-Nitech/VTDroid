{
    "redirect_to": "",
    "target_source": "imei",
    "static_bytecode_instrumentation": {
        "flash_interval": 1,
        "aapt": "aapt",
        "jarsigner": "jarsigner",
        "apktool": {
            "name": "apktool",
            "p_opt": "--use-aapt2"
        }
    },
    "information_flow_analysis": {
        "timeout": "60 * 60",
        "mem_limit": "1024 * 1024 * 1024 * 10",
        "lines_limit": "-1",
        "follow_goto_limit": 6,
        "ocr_cmd": "tesseract"
    }
}
